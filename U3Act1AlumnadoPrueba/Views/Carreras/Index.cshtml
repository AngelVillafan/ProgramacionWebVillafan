@model IEnumerable<U3Act1AlumnadoPrueba.Models.Carrera>
<html>
<head>
    <title>Carreras</title>
</head>
<body>

    <h1>Lista de carreras</h1>

    <table border="1">
        <thead>
            <tr>
                <th>Clave</th>
                <th>Nombre</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var carrera in Model)
            {
                <tr>
                    <td>@(carrera.Clave)</td>
                    <td>@(carrera.Nombre)</td>
                </tr>
            }
           
        </tbody>
    </table>























</body>


</html>